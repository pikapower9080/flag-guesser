{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": [
        "scripts/*",
        "webpack.config.js"
    ],
    "rules": {
        "no-unused-vars": ["warn", { "vars": "all", "args": "none" }]
    }
}
